{
  "name": "libra-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "lint": "eslint --fix --ext .js src",
    "serve": "vue-cli-service serve",
    "dev:st": "vue-cli-service serve --mode public",
    "dev:uat": "npm run test",
    "docker": "docker-compose up -d --build --force-recreate",
    "build": "vue-cli-service build --mode dev",
    "build:dev": "vue-cli-service build --mode dev",
    "build:st": "vue-cli-service build --mode st",
    "build:uat": "vue-cli-service build --mode uat",
    "build:prod": "vue-cli-service build --mode prod",
    "new-page": "node ./build/bin/newPage.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "crypto-js": "^3.1.9-1",
    "element-ui": "^2.12.0",
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vue-cropperjs": "^3.0.0",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint-config-airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}